  delay:60*1000,
  change:function(s){
    if(s!=stk_app.stk){
      stk_app.stk=s;
      stk_app.load();
      //stk_app.smartbox && (stk_app.smartbox.typeIpt.value = s);
    }
  },
  load:function(){
    clearTimeout(stk_app.time);
    var lx=stk_app.stk,o;
    o=stk_app.$('stocknum_img_'+lx);if(o)o.src="http://img.gtimg.cn/images/hq_parts_little8/"+stk_app.img[lx]+"?"+Math.random();
    __.load("http://qt.gtimg.cn/"+Math.random()+"&q="+stk_app.map[lx],function(){
      if(window["v_"+stk_app.map[lx]]){
        var d=window["v_"+stk_app.map[lx]].split("~"),o,c,t;
        if(d[4]<0){c="green",t=""}else if(d[4]>0){c="red",t="+"}else {c=t=""}
        o=stk_app.$('stocknum_hq0_'+lx);
        if(o){o.innerHTML=d[3];o.className=c;}
        o=stk_app.$('stocknum_hq1_'+lx);
        if(o){o.innerHTML=t+d[4];o.className=c;}
        o=stk_app.$('stocknum_hq2_'+lx);
        if(o){o.innerHTML=t+d[5]+"%";o.className=c;}
        window["v_"+stk_app.map[lx]] = null;
      }
    });
    stk_app.time=setTimeout(function(){stk_app.load();},stk_app.delay);
  },
  timer:null,
  tagOver:function(o){
    var target = o.id && o.id.match(__.Lang.getReg('stocknum_(\\d)'));
    if (stk_app.timer)clearTimeout(stk_app.timer);
    stk_app.timer = setTimeout(function(){
      try{
        __.Lang.e(__.Dom.$("#stocknum div"),function(obj){
          obj.className = obj==o?'s':'d';
        });
        __.Lang.e(__.Dom.$("#stocknumInfo > div"),function(obj){
          obj.className = obj.id.split('_')[1] == target[1]?'dis':'undis';
        });
      }catch(e){}
    },100);      
  },
  init:function(){
    //注册
    var t,o;
     __.Event.on(__.Dom.$("#stocknum div"), "mouseover", function(e){            
       e = e || window.event;
       var target = e.target || e.srcElement,o=target;
       target = target.id && target.id.match(__.Lang.getReg('stocknum_(\\d)'));
       if (target && target[1]) {
         stk_app.change(stk_app.ind[target[1]]);stk_app.tagOver(o);
       }
     });
     //晚9-早9
     t='14';
     if(t<9||t>=21){
       stk_app.stk='AS';
       stk_app.tagOver(stk_app.$("stocknum_2"));
     }
     //
     stk_app.load();

     //smartbox
     stk_app.smartbox = new __.widget.SmartBox('container-GP',{
       defaultType : 'ALL',
       showPLink : false,
       showTitle : false,
       showType : false
     });
     var theUlS = __.Dom.f('#'+stk_app.smartbox.resultCtnId+' ul').style;
     theUlS.display = '';
     theUlS.lineHeight = '15px';
     //css
     var L=['GP','HK','AS'],o,i,a,j;
     o=document.getElementById("container-GP").childNodes[0].childNodes[0];
     if(o){
       a=o.childNodes;
       for(j=0;j<a.length;j++){
         o=a[j].childNodes[0];
         if(o.className=="du-sx-query") {
           o.className="w";
           o.style.color="#5F5F5F";
           o.style.border="1px solid #7F9DB9"
           o.style.lineHeight="14px"
           o.style.padding="1px";
           o.style.width="94px";
           o.style.fontSize="12px";
           //o.style.marginright="0";
           o.style.cssText="";o.cssText="";
         }else if(o.className=="du-sx-submit"){
           o.className="i";
           o.style.cursor="pointer";
           o.style.height="18px";
           o.style.lineHeight="14px"
           o.style.paddingTop="1px";
           o.style.paddingLeft="0";
           o.style.width="34px";
           o.style.border="1px solid #C2C2C0";
           o.style.backgroundImage="url()";
           o.style.cssText="";o.cssText="";
           o.style.background="#FFFFFF none repeat scroll 0 0";
           o.parentNode.style.marginRight="auto";
           o.parentNode.style.paddingTop="0";
           o.parentNode.style.paddingLeft="5px";
         }
       }
    }
  }
};
__.onReady(function(){stk_app.init();});
