var Browser = new Object();

Browser.ua = window.navigator.userAgent.toLowerCase();
Browser.ie = /msie/.test(Browser.ua);
Browser.moz = /gecko/.test(Browser.ua);

//JsLoader
var JsLoader = {
	load: function(sUrl, fCallback)
	{
		var _script = document.createElement("script");
		_script.setAttribute("type", "text/javascript");
		_script.setAttribute("src", sUrl);
		document.getElementsByTagName("head")[0].appendChild(_script);

		if (Browser.ie)
		{
			_script.onreadystatechange = function()
			{
				if (this.readyState=="loaded" || this.readyState=="complete")
				{
					fCallback();
				}
			};
		}
		else if (Browser.moz)
		{
			_script.onload = function()
			{
				fCallback();
			};
		}
		else
		{
			fCallback();
		}
	}
};
</script>
<script>
	function loadAstroYs(objectID)
	{
		if (objectID == "" || objectID == null)
		{
			return;
		}
		var astroID = document.getElementById(objectID).value;

		if (astroID == "" || astroID == "0")
		{
			return;
		}

		var now = new Date();
		var year = now.getFullYear();
		var month = now.getMonth() + 1;
		var day = now.getDate();
		
		var randStr = "" + now.getFullYear() + "" + (now.getMonth() + 1) + "" + now.getDate();

		var urljs = "http://data.astro.qq.com/infopic/" + astroID + "/todayAstro.js?" + randStr;

		JsLoader.load(urljs, function(){});
	}

	function astroYsSearch(type,objectID)
	{
		var astroID = document.getElementById(objectID).value;

		if (astroID == "" || astroID == "0")
		{
			alert("请选择星座");
			return;
		}

		var now = new Date();
		var year = now.getFullYear();
		var month = now.getMonth() + 1;
		var day = now.getDate();
		
		//存储星座相关的信息
		var astroInfo = {
			"Today" : "dayastro",
			"Week" : "weekastro"
		};

		var randStr = "" + now.getFullYear() + "" + (now.getMonth() + 1) + "" + now.getDate();

		var urljs = "http://data.astro.qq.com/infopic/" + astroID + "/todayAstro.js?" + randStr;

		if (typeof todayAstro == "undefined")
		{
			JsLoader.load(urljs, function()
			{
				var urlPage = "http://data.astro.qq.com/astro/crash.shtml";
				if (typeof todayAstro != "undefined")
				{
					var FID = todayAstro[type + "FID"];
					if (FID != null && FID != "")
					{
						urlPage = "http://data.astro.qq.com/" + astroInfo[type] + "/" + Math.floor(FID / 1000);
						urlPage += "/" + FID + "/index.shtml";		
					}
				}
				window.open(urlPage);
			});
		}else
		{
			var urlPage = "http://data.astro.qq.com/astro/crash.shtml";

			var FID = todayAstro[type + "FID"];

			if (FID != null && FID != "")
			{
				urlPage = "http://data.astro.qq.com/" + astroInfo[type] + "/" + Math.floor(FID / 1000);
				urlPage += "/" + FID + "/index.shtml";
			}

			window.open(urlPage);
		}
	}

	function czodYsSearch(type,objectID)
	{
		if (objectID == "" || objectID == null)
		{
			return;
		}
		var czodID = document.getElementById(objectID).value;

		if (czodID == "" || czodID == "0")
		{
			alert("请选择生肖");
            return;
		}

		var now = new Date();
		var year = now.getFullYear();
		var month = now.getMonth() + 1;
		var day = now.getDate();
		
		var czodInfo = {
			"Week" : "weekczod",
			"Month" : "monthczod"
		};

		var randStr = "" + now.getFullYear() + "" + (now.getMonth() + 1) + "" + now.getDate();

		var urljs = "http://data.astro.qq.com/czodinfo/" + czodID + "/todayCzod.js?" + randStr;

		if (typeof todayCzod == "undefined")
		{
			JsLoader.load(urljs, function()
			{
				var urlPage = "http://data.astro.qq.com/astro/crash.shtml";

				if (typeof todayCzod != "undefined")
				{
					var FID = todayCzod[type + "FID"];
					if (FID != null && FID != "")
					{
						urlPage = "http://data.astro.qq.com/" + czodInfo[type] + "/" + Math.floor(FID / 1000);
						urlPage += "/" + FID + "/index.shtml";
					}
				}
				window.open(urlPage);
			});
		}else
		{
			var urlPage = "http://data.astro.qq.com/astro/crash.shtml";
			var FID = todayCzod[type + "FID"];
			if (FID != null && FID != "")
			{
				urlPage = "http://data.astro.qq.com/" + czodInfo[type] + "/" + Math.floor(FID / 1000);
				urlPage += "/" + FID + "/index.shtml";
			}

			window.open(urlPage);
		}
	}
	
	function loadCzodYs(objectID)
	{
		if (objectID == "" || objectID == null)
		{
			return;
		}
