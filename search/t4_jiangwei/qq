<script>

/*<![CDATA[*/
                        
var now = new Date(2010, 08-1, 02, 18, 50, 58);
//var now = new Date();

var grt= new Date("05/01/2010 00:00:00");

/*]]>*/  

function createtime(){

now.setTime(now.getTime()+250);
days = (grt - now) / 1000 / 60 / 60 / 24;
dnum = Math.floor(days);
hours = (grt - now) / 1000 / 60 / 60 - (24 * dnum);
hnum = Math.floor(hours);
if(String(hnum).length ==1 ){hnum =  hnum;}
minutes = (grt - now) / 1000 /60 - (24 * 60 * dnum) - (60 * hnum);
mnum = Math.floor(minutes);
if(String(mnum).length ==1 ){mnum = "0" + mnum;}
seconds = (grt - now) / 1000 - (24 * 60 * 60 * dnum) - (60 * 60 * hnum) - (60 * mnum);
snum = Math.round(seconds);
if(String(snum).length ==1 ){snum = "0" + snum;}

if(dnum > 0){
	dnum = dnum + 1;
	document.getElementById("timeup").innerHTML = "距世博开幕<span class='nums'>"+dnum+"</span>天";
	}else if(dnum == 0){
		if( hnum > 3){
		hnum = hnum - 3;
		document.getElementById("timeup").innerHTML = "距世博开幕还有<span class='nums'>"+hnum + "</span>小时" ;
		}
		else{
			document.getElementById("timeup").innerHTML = "<span class='lchot'>上海世博会盛大开幕</span>" ;
			}
		 
		}else{
			dnum = Math.abs(dnum);
			document.getElementById("timeup").innerHTML = dnum;
			}
}

setInterval("createtime()",250);     
         
</script>
