function getExpires(a){//a:day
	var expires=new Date(new Date().getTime()+(a?a:1)*24*3600*1000);
	return expires;
}

function setdefSkin(){
	var expires = getExpires(90);
	var n =  MiniSite.Cookie.get("qq_index_skin");
	if(n!=null){
		call_0410("UNKNOWN","skin",n);
		var m = MiniSite.Cookie.get("qq_index_skin_20100709");//qq_index_skin_def
		if(!m){
			setSkin(0);//0
			MiniSite.Cookie.set("qq_index_skin_20100709","true",expires);
		}else{
			setSkin(n);
		}
			
	}else{
		call_0410("UNKNOWN","skin",0);
		MiniSite.Cookie.set("qq_index_skin_20100709","true",expires);
	}
}

	function setdef(){
		var n=MiniSite.Cookie.get("qq_index_ip_1hrcache");
		if(!n) n=MiniSite.Home._ip?MiniSite.Home._ip:null;
		if(n!=null){
			//alert(MiniSite.Home.Provinc)
			var p=n.split(",")[0];
			n=MiniSite.Home.Provinc[p];
			if(_wip.test(n)){
				$("tailorForIP").style.display="block";
				tail(n);
			}
		}else{
			MiniSite.Home.print()
		}
	}
