<script type="text/javascript">
function showPPInfoMenu(num){
	for(var i=0;i<3;i++){
		$("ppMenu"+i).className="";
		$("ppMainDesc"+i).style.display="none";
	}
	$("ppMenu"+num).className="ppMenuHere";
	$("ppMainDesc"+num).style.display="";
}	
showPPInfoMenu(0);
var ppRollTime=1;
function callPPRoll(){
	if(ppRollTime<15){
		showPPInfoMenu(ppRollTime%3);
		ppRollTime++;
	}else clearInterval(ppRoll);
}
var ppRoll=setInterval("callPPRoll()",6000);
</script><!--[if !IE]>|xGv00|67d5840b049b32dd9e6ee3ee71db4f5b<![endif]-->
